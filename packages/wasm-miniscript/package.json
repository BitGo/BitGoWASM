{
  "name": "@bitgo/wasm-miniscript",
  "version": "0.1.0",
  "lint-staged": "MIT",
  "files": [
    "js/wasm/wasm_miniscript_bg.wasm",
    "js/wasm/wasm_miniscript.js",
    "js/wasm/wasm_miniscript_bg.js",
    "js/wasm/wasm_miniscript.d.ts",
    "js/index.js"
  ],
  "main": "js/index.js",
  "types": "js/index.d.ts",
  "sideEffects": [
    "./js/wasm/wasm_miniscript.js"
  ],
  "browser": {
    "./js/index.js": "./js/browser/index.js",
    "./js/wasm/wasm_miniscript_bg.wasm": "./js/browser/wasm/wasm_miniscript_bg.wasm",
    "./js/wasm/wasm_miniscript.js": "./js/browser/wasm/wasm_miniscript.js",
    "./js/wasm/wasm_miniscript_bg.js": "./js/browser/wasm/wasm_miniscript_bg.js",
    "./js/wasm/wasm_miniscript.d.ts": "./js/browser/wasm/wasm_miniscript.d.ts"
  },
  "scripts": {
    "test": "mocha --recursive test",
    "build:wasm-nodejs": "wasm-pack build --out-dir js/wasm --no-pack --target nodejs --weak-refs",
    "build:wasm-browser": "wasm-pack build --out-dir js/browser/wasm --no-pack --target browser --weak-refs",
    "build:wasm": "npm run build:wasm-nodejs && npm run build:wasm-browser",
    "build:ts-browser": "tsc --module es2020 --target es2020 --outDir js/browser",
    "build:ts": "tsc && npm run build:ts-browser",
    "build": "npm run build:wasm && npm run build:ts"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.14.10",
    "mocha": "^10.6.0",
    "ts-node": "^10.9.2"
  }
}
